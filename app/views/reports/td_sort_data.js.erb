var chart = $('#left-chart').highcharts();
chart.xAxis[0].setCategories(<%= @data.map(&:name).to_json.html_safe %>, false);
chart.series[0].update({ 
  name: "<%= @sort %>",
  data: [ <% @data.each do |d| %>
  {
    y: <%= d.y %>,
    id: "<%= d.id %>"
  },
  <% end %> ]
}, false);

chart.redraw();
